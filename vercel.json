{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/enviar-factura",
            "destination": "/api/enviar-factura.js"
        },
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ],
    "env": {
        "NODE_ENV": "production"
    }
}